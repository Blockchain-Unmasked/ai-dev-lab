# AI/DEV Lab App - Environment Configuration Example
# ================================================
# Copy this file to .env and fill in your actual values
# NEVER commit .env files to version control

# =============================================================================
# APP CONFIGURATION
# =============================================================================
APP_NAME=AI_DEV_LAB_DEMO
APP_VERSION=2.0.0
APP_ENV=development
APP_DEBUG=true
APP_SECRET_KEY=your-super-secret-key-here
APP_HOST=localhost
APP_PORT=8000

# =============================================================================
# DATABASE CONFIGURATION
# =============================================================================
DATABASE_TYPE=sqlite
DATABASE_URL=sqlite:///app/database/demo_database.db
DATABASE_HOST=localhost
DATABASE_PORT=5432
DATABASE_NAME=ai_dev_lab_demo
DATABASE_USER=your_db_user
DATABASE_PASSWORD=your_db_password
DATABASE_SSL_MODE=prefer

# =============================================================================
# AI API CONFIGURATION
# =============================================================================
# Google Gemini API
GEMINI_API_KEY=your-gemini-api-key-here
GEMINI_MODEL=gemini-pro
GEMINI_TEMPERATURE=0.7
GEMINI_MAX_TOKENS=2048

# OpenAI API (Alternative)
OPENAI_API_KEY=your-openai-api-key-here
OPENAI_MODEL=gpt-4
OPENAI_TEMPERATURE=0.7
OPENAI_MAX_TOKENS=2048

# Anthropic Claude API (Alternative)
ANTHROPIC_API_KEY=your-anthropic-api-key-here
ANTHROPIC_MODEL=claude-3-sonnet-20240229
ANTHROPIC_MAX_TOKENS=4096

# =============================================================================
# MCP SERVER CONFIGURATION
# =============================================================================
MCP_SERVER_HOST=localhost
MCP_SERVER_PORT=8001
MCP_SERVER_TIMEOUT=30
MCP_SERVER_RETRY_ATTEMPTS=3

# Lab MCP Server
LAB_MCP_SERVER_URL=http://localhost:8002
LAB_MCP_SERVER_API_KEY=your-lab-mcp-api-key

# App MCP Server
APP_MCP_SERVER_URL=http://localhost:8003
APP_MCP_SERVER_API_KEY=your-app-mcp-api-key

# =============================================================================
# SECURITY CONFIGURATION
# =============================================================================
SECURITY_ENABLED=true
SECURITY_BCRYPT_ROUNDS=12
SECURITY_JWT_SECRET=your-jwt-secret-here
SECURITY_JWT_EXPIRY=3600
SECURITY_SESSION_TIMEOUT=1800

# CORS Configuration
CORS_ALLOWED_ORIGINS=http://localhost:3000,http://localhost:8000
CORS_ALLOWED_METHODS=GET,POST,PUT,DELETE,OPTIONS
CORS_ALLOWED_HEADERS=Content-Type,Authorization

# =============================================================================
# LOGGING CONFIGURATION
# =============================================================================
LOG_LEVEL=INFO
LOG_FORMAT=json
LOG_FILE=app/logs/app.log
LOG_MAX_SIZE=10485760
LOG_BACKUP_COUNT=5

# =============================================================================
# QUEUE SYSTEM CONFIGURATION
# =============================================================================
QUEUE_SYSTEM_ENABLED=true
QUEUE_MAX_CONCURRENT_SESSIONS=10
QUEUE_SESSION_TIMEOUT=300
QUEUE_PRIORITY_LEVELS=5

# =============================================================================
# AGENT SYSTEM CONFIGURATION
# =============================================================================
AGENT_SYSTEM_ENABLED=true
AGENT_MAX_TIERS=4
AGENT_ESCALATION_ENABLED=true
AGENT_QA_ENABLED=true
AGENT_STEALTH_MODE_ENABLED=true

# =============================================================================
# PROMPT ENGINE CONFIGURATION
# =============================================================================
PROMPT_ENGINE_ENABLED=true
PROMPT_ENGINE_MODE=enhanced
PROMPT_ENGINE_TEMPLATES_PATH=app/meta/templates/
PROMPT_ENGINE_SCHEMAS_PATH=app/meta/schemas/

# =============================================================================
# MONITORING AND ANALYTICS
# =============================================================================
MONITORING_ENABLED=true
METRICS_COLLECTION_ENABLED=true
PERFORMANCE_MONITORING_ENABLED=true
ERROR_TRACKING_ENABLED=true

# =============================================================================
# EXTERNAL SERVICES
# =============================================================================
# Web Scraping Services
SCRAPING_ENABLED=true
SCRAPING_RATE_LIMIT=10
SCRAPING_USER_AGENT=Mozilla/5.0 (AI/DEV Lab Bot)

# File Storage
STORAGE_TYPE=local
STORAGE_PATH=app/storage/
STORAGE_MAX_SIZE=1073741824

# =============================================================================
# DEVELOPMENT AND TESTING
# =============================================================================
TESTING_ENABLED=true
TEST_DATABASE_URL=sqlite:///app/database/test_database.db
MOCK_AI_RESPONSES=true
DEBUG_MODE=true

# =============================================================================
# DEPLOYMENT CONFIGURATION
# =============================================================================
DEPLOYMENT_ENVIRONMENT=development
DEPLOYMENT_REGION=us-west-2
DEPLOYMENT_VERSION=v2.0.0

# =============================================================================
# NOTIFICATIONS AND ALERTS
# =============================================================================
NOTIFICATIONS_ENABLED=true
EMAIL_NOTIFICATIONS_ENABLED=false
SLACK_NOTIFICATIONS_ENABLED=false
WEBHOOK_NOTIFICATIONS_ENABLED=false

# =============================================================================
# BACKUP AND RECOVERY
# =============================================================================
BACKUP_ENABLED=true
BACKUP_SCHEDULE=daily
BACKUP_RETENTION_DAYS=30
BACKUP_PATH=app/backups/

# =============================================================================
# CUSTOM CONFIGURATION
# =============================================================================
# Add any custom configuration variables below
CUSTOM_FEATURE_FLAG=false
CUSTOM_API_ENDPOINT=https://api.example.com
CUSTOM_TIMEOUT=60
